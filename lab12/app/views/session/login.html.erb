<h1>Log In</h1>
<%= form_tag url_for(controller: :session, action: :create), method: 'post' do |form| %>
    <div class="field">
        <%= label_tag :username %>
        <%= text_field_tag :username %>
    </div>
    <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>
    <div class="actions">
        <%= submit_tag "Log In" %>
    </div>
<% end %>
<%= link_to 'Зарегистрироваться', new_user_path %>